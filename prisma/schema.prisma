generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider     = "mysql"
    url          = env("DATABASE_URL")
    relationMode = "prisma"
}

// ignore this for now, till the schema is finalized.

model Users {
    id              String    @id @default(uuid())
    emailVerified   Boolean? @default(false)
    accountVerified Boolean? @default(false)
    email           String   @unique
}
